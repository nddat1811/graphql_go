# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  id: ID!
  name: String!
  email: String!
  password: String!
}

type Query {
  getAllUsers: [User!]!
}

input NewUser {
  name: String!
  password: String!
  email: String!
}

type Mutation {
  createNewUser(input: NewUser!): User!
}
